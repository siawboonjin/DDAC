@model IEnumerable<WebApplication3.Models.ships>

@{
    ViewBag.Title = "Ship";
}

<h2>Ship</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ship_num)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ship_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ship_num)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ship_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
